{
  "name": "tsnocde-clean-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ciro-stodulski/tsnocde-clean-api.git"
  },
  "homepage": "https://github.com/ciro-stodulski/tsnocde-clean-api#readme",
  "scripts": {
    "start": "node  ./src/main/index.js",
    "dev": "tsnd ./src/main/index.ts --transpile-only",
    "build": "npm i && rimraf dist && tsc -p .",
    "test": "nyc mocha --exit --config=.mocha.yml './src/**/*.spec.ts'",
    "lint": "eslint  'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "migration:create": "knex migrate:make",
    "migration:latest": "knex migrate:latest",
    "migration:rollback": "knex migrate:rollback"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "all": true,
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "dependencies": {
    "amqplib": "^0.10.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "chai": "^4.3.4",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "ioredis": "^5.0.6",
    "joi": "^17.6.0",
    "knex": "^0.19.5",
    "mysql2": "^2.3.3",
    "nock": "^13.2.7",
    "sinon": "^11.1.1",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/chai": "^4.2.18",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/mocha": "^8.2.2",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "ts-node-dev": "^2.0.0-0"
  }
}
